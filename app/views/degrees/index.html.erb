<p id="notice"><%= notice %></p>

<h1>Listing Degrees</h1>

<table>
<tr>
  <th>userId</th>
  <th>id</th>
  <th>title</th>
  <th>body</th>
</tr>
<% @parsed.each do |p| %>
<tr>
  <td><%= p["userId"] %></td>
  <td><%= p["id"] %></td>
  <td><%= p["title"] %></td>
  <td><%= p["body"] %></td>
</tr>
<% end %>
</table>


<br />
<br />
<br />
<br />
<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @degrees.each do |degree| %>
      <tr>
        <td><%= link_to 'Show', degree %></td>
        <td><%= link_to 'Edit', edit_degree_path(degree) %></td>
        <td><%= link_to 'Destroy', degree, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

