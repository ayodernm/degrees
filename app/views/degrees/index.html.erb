<p id="notice"><%= notice %></p>

<h1>Listing Degrees</h1>

<ul>
  <% @parsed.each do |event| %>
      <% event.each do |e| %>
      <li><%= e.first %></li>
      <br />
       <% e.drop(1).each do |f| %>
       <br />
       <table>
          <tr>
           <th>name</th>
           <th>description</th>
           <th>prerequistes</th>
           <th>hours</th>
          </tr>
       <% f.each do |g| %>
          <tr>
            <td><%= g["name"] %></td>
            <td><%= g["description"] %></td>
            <td><%= g["prerequisites"] %></td>
            <td><%= g["hours"] %></td>
          </tr>
      <% end %>
       </table>
       <br />
       <br />
      <% end %>
     <% end %>  
  <% end %>
</ul>



<br />
<br />
<br />
<br />
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @degrees.each do |degree| %>
      <tr>
        <td><%= link_to 'Show', degree %></td>
        <td><%= link_to 'Edit', edit_degree_path(degree) %></td>
        <td><%= link_to 'Destroy', degree, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Degree', new_degree_path %>
